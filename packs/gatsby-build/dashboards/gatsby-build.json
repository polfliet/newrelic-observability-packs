          "title": "Average Duration by API (ms)",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "query": "SELECT average(`duration.ms`) FROM Span SINCE 24 hours ago FACET api TIMESERIES"
